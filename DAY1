#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[10001];

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int pos, x;
    scanf("%d", &pos);
    scanf("%d", &x);

    if (pos < 1 || pos > n + 1) {
        return 0;
    }

    for (int i = n; i > pos - 1; i--) {
        arr[i] = arr[i - 1];
    }

    arr[pos - 1] = x;

    for (int i = 0; i <= n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
